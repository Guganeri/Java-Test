package prgram;

public class Test {

	public static void main(String[] args) {
			
		
		double [] vect = new double[7];
		
		vect[0] = 1;
		vect[1] = 2;
		vect[2] = 3;
		vect[4] = 4;
		vect[5] = 2;
		vect[6] = 1;
		vect[7] = 2;
		
		for(int i=0; i < vect.length; i++) {
			if (vect[i-1] == vect[i-2]) {
				if(vect[i]== vect[i-2]) {
					System.out.println("Same thing");
				}
			}
			
		}
		

	}

}
